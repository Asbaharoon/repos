<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="SalIncome" language="groovy" pageWidth="166" pageHeight="409" columnWidth="166" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0" >
	<parameter name="month" class="java.lang.Integer"/>
	<parameter name="year" class="java.lang.Integer"/>
	<parameter name="emp_code" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
     salary_data.`sd_amount` AS salary_data_sd_amount,
     salary_head_master.`sh_name` AS salary_head_master_sh_name
FROM
      salary_data left JOIN salary_head_master
ON salary_head_master.`sh_id` = salary_data.`sd_head_code` where  month(sd_date) = $P{month}
and year(sd_date) = $P{year} and sh_type=1 and sd_emp_code=$P{emp_code}]]>
	</queryString>
	<field name="salary_data_sd_amount" class="java.lang.Integer">
		<fieldDescription><![CDATA[]]></fieldDescription>
	</field>
	<field name="salary_head_master_sh_name" class="java.lang.String">
		<fieldDescription><![CDATA[]]></fieldDescription>
	</field>
	<variable name="totalS" class="java.lang.Integer" calculation="Sum">
		<variableExpression><![CDATA[$F{salary_data_sd_amount}]]></variableExpression>
	</variable>
	<background>
		<band splitType="Stretch"/>
	</background>
	<detail>
		<band height="16" splitType="Stretch">
			<textField>
				<reportElement x="0" y="0" width="110" height="15"/>
				<box leftPadding="4"/>
				<textElement verticalAlignment="Middle">
					<font size="8"/>
				</textElement>
				<textFieldExpression><![CDATA[$F{salary_head_master_sh_name}]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement x="120" y="0" width="50" height="15"/>
				<box>
