<struts>
    <!-- Add your configuration elements here -->
    <package name="org.IGNOU.ePortfolio.Assignment" namespace="/Assignment" extends="struts-default">
        <action name="QuestionSet" class="org.IGNOU.ePortfolio.Assignment.AssignmentQTypeAction">
            <result name="success">QuestionSet.jsp</result>
        </action>
        <action name="CreateAssignment" class="org.IGNOU.ePortfolio.Assignment.AddAssignment">
            <result name="success">MyAssignment.jsp</result>
        </action>
        <action name="RubricSetupAction" class="org.IGNOU.ePortfolio.Assignment.RubricAction">
            <result name="success">RubricSetup.jsp</result>
        </action>
    </package>
</struts>
