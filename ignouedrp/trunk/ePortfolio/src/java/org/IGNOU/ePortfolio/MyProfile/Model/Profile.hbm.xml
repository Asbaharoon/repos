<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  
  <class catalog="eportfolio" name="org.IGNOU.ePortfolio.MyProfile.Model.ProfilePersonal" table="profile_personal">
    <id name="personalInfo" type="long">
      <column name="personalInfo"/>
      <generator class="increment"/>
    </id>
    <property name="userId" type="string">
      <column length="100" name="user_id"/>
    </property>
    <property name="fbook" type="string">
      <column length="65535" name="fBook"/>
    </property>
    <property name="ftvshow" type="string">
      <column length="65535" name="fTVShow"/>
    </property>
    <property name="fmovie" type="string">
      <column length="65535" name="fMovie"/>
    </property>
    <property name="fquote" type="string">
      <column length="65535" name="fQuote"/>
    </property>
    <property name="oinfo" type="string">
      <column length="65535" name="oInfo"/>
    </property>
  </class>
  <class catalog="eportfolio" name="org.IGNOU.ePortfolio.MyProfile.Model.ProfileContact" table="profile_contact">
    <id name="contactInfoId" type="long">
      <column name="contactInfo_id"/>
      <generator class="increment"/>
    </id>
    <property name="userId" type="string">
      <column length="100" name="user_id"/>
    </property>
    <property name="address1" type="string">
      <column length="100" name="address1"/>
    </property>
    <property name="address2" type="string">
      <column length="100" name="address2"/>
    </property>
    <property name="city" type="string">
      <column length="50" name="city"/>
    </property>
    <property name="state" type="string">
      <column length="50" name="state"/>
    </property>
    <property name="country" type="string">
      <column length="100" name="country"/>
    </property>
    <property name="pin" type="java.lang.Integer">
      <column name="pin"/>
    </property>
    <property name="htelephone" type="java.lang.Long">
      <column name="hTelephone"/>
    </property>
    <property name="otelephone" type="java.lang.Long">
      <column name="oTelephone"/>
    </property>
    <property name="mobile" type="java.lang.Long">
      <column name="mobile"/>
    </property>
    <property name="fax" type="java.lang.Long">
      <column name="fax"/>
    </property>
    <property name="email1" type="string">
      <column length="100" name="email1"/>
    </property>
    <property name="email2" type="string">
      <column length="100" name="email2"/>
    </property>
    <property name="email3" type="string">
      <column length="100" name="email3"/>
    </property>
    <property name="owebsite" type="string">
      <column length="65535" name="oWebsite"/>
    </property>
    <property name="pwebsite" type="string">
      <column length="65535" name="pWebsite"/>
    </property>
  </class>
  <class catalog="eportfolio" name="org.IGNOU.ePortfolio.MyProfile.Model.ProfileBasic" table="profile_basic">
    <id name="basicInfoId" type="long">
      <column name="basicInfo_ID"/>
      <generator class="increment"/>
    </id>
    <property name="userId" type="string">
      <column length="100" name="user_id"/>
    </property>
    <property name="fname" type="string">
      <column length="45" name="fName"/>
    </property>
    <property name="mname" type="string">
      <column length="45" name="mName"/>
    </property>
    <property name="lname" type="string">
      <column length="45" name="lName"/>
    </property>
    <property name="gender" type="string">
      <column length="10" name="gender"/>
    </property>
    <property name="dateOfBirth" type="string">
      <column length="45" name="date_of_birth"/>
    </property>
    <property name="pbirth" type="string">
      <column length="100" name="pBirth"/>
    </property>
    <property name="mstatus" type="string">
      <column length="45" name="mStatus"/>
    </property>
    <property name="aboutMe" type="string">
      <column length="65535" name="aboutMe"/>
    </property>
  </class>
</hibernate-mapping>
