<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 1.7.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/1.7.0/distro-source/core/src/gwt-module.dtd">
<module rename-to='admissionsystem'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
 <inherits name='com.google.gwt.user.User'/>
 <inherits name='com.gwtext.GwtExt'/>
 <inherits name="com.google.gwt.i18n.I18N"/>
<inherits name="com.gwtextux.GwtExtUx"/>
 <inherits name="com.smartgwt.SmartGwt"/>
 
 <extend-property name="locale" values="messages"/>
  <extend-property name="locale" values="dummy"/>
  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
  <!-- <inherits name='com.google.gwt.user.theme.standard.Standard'/> -->
  <inherits name='com.google.gwt.user.theme.chrome.Chrome'/>
  <stylesheet src="themes/silverCherry/css/xtheme-silverCherry.css"/>
  <!--   <inherits name='com.google.gwt.user.theme.dark.Dark'/> -->    

  <!-- Other module inherits                                      -->

  <!-- Specify the app entry point class.                         -->
  <entry-point class='in.ac.dei.edrp.client.Login.Login'/>
</module>
