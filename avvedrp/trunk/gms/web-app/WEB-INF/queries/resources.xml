<?xml version="1.0"?>
 <Schema name="Resources">
 <Cube name="Resources">
 <Table name="Resources"/> 
 	 
 	     
     <Dimension name="Project" hasAll="true">
            <Hierarchy hasAll="true">
            			<Level name="Project ID" column="project_code" type="String" uniqueMembers="false"/> 
                      <Level name="ProjectName" column="project_name" type="String" uniqueMembers="false"/> 
                               
            </Hierarchy>
     </Dimension>
     <Dimension name="Institute" hasAll="false">
            <Hierarchy hasAll="true" allMemberName="All Institute">
            	<Level name="Institute"  column="institute" type="String" uniqueMembers="false"/>
                       
    		</Hierarchy>
     </Dimension>
     <Dimension name="Resource Type" hasAll="false">
            <Hierarchy hasAll="true" >
            	<Level name="ResourceType"  column="Resource_Type" type="String" uniqueMembers="false"/>
                       
    		</Hierarchy>
     </Dimension>
    
         
    	<Measure name="No Of Resources" column="No_of_resources" aggregator="sum" formatString="#,###"/>
         <Measure name="Cost" column="Cost" aggregator="sum" formatString="#,###"/>
         
       
    </Cube>
    </Schema>