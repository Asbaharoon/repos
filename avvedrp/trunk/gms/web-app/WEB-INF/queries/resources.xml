<?xml version="1.0"?>
 <Schema name="Resources">
 <Cube name="Resources">
 <Table name="Resources"/> 
 	 
 	     
     <Dimension name="ProjectName">
            <Hierarchy hasAll="true">
            		 <Level name="ProjectName" column="ProjectName" type="String" uniqueMembers="false"/> 
                               
            </Hierarchy>
     </Dimension>
     <Dimension name="Institute">
      <Hierarchy hasAll="true">
           	<Level name="Institute"  column="Inst" type="String" uniqueMembers="false"/>
                       
    		</Hierarchy>
     </Dimension>
     
     <Dimension name="Resource Type">
      <Hierarchy hasAll="true" >
            	<Level name="ResourceType"  column="designation" type="String" uniqueMembers="false"/>
                       
     </Hierarchy>
     </Dimension>
  		<Measure name="No of Resources" column="EmpNmbr" aggregator="count" formatString="#,###"/>
  		
        
    </Cube>
    </Schema>
