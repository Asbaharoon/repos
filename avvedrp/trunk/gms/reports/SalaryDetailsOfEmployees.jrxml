	<queryString><![CDATA[SELECT 
P.name AS projectName,
PE.emp_name AS employeeName,
ED.designation AS designation,
SC.name AS salComponent,
PES.salary_amount,
PES.with_effect_from AS fromDate

FROM
projects P,
project_employee PE,
employee_designation ED,
project_employee_salary_details PES,
salary_component SC

WHERE
PES.project_employee_id = PE.id
AND PE.employee_designation_id = ED.id
AND PES.salary_component_id = SC.id
AND PE.projects_id = P.id
AND P.id = $P{projectID}
AND PES.status = 'Y'
GROUP BY PES.id]]></queryString>

