	<queryString><![CDATA[SELECT 
	YEAR(PR.project_start_date) AS YEAR,
	(SELECT (CONCAT(I.name," ",I.user_sur_name)) FROM investigator I,projectspimap PMAP WHERE I.id=PMAP.investigator_id AND PMAP.projects_id=PR.id AND PMAP.role='PI' GROUP BY PR.id) AS PI,
	PR.name AS ProjectTitle,
	P.code AS Grantor,
	PD.name AS Department,
	PD.department_code AS CODE,
       ( (YEAR(CURRENT_DATE)- YEAR(PR.project_start_date))+1 ) AS CurrentYear,
       PT.type AS ProjectType
FROM 
	projects PR,
	grant_allocation GA,
	party P,
	party_department PD,
	project_department_map PM,
	project_type PT
WHERE 
	P.id =GA.party_id
	AND P.id=PD.party_id
	AND PD.id = PM.party_department_id
	AND PR.id = PM.projects_id
	AND P.id = $P{partyID}
	AND PR.project_type_id = PT.id
GROUP BY 
	PR.id]]></queryString>

