	<parameter name="projectID" isForPrompting="true" class="java.lang.String"/>
	<parameter name="reportDate" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT 
	P.name AS ProjectTitle,
	P.code AS IDNO,
	AH.name AS SanctionedHead,
	MONTHNAME(GES.date_of_expense) AS MONTH,
	(SELECT SUM(GR.amount) FROM grant_receipt GR,grant_allocation GA,grant_allocation_split GS,account_heads AHS WHERE GA.projects_id=GR.projects_id AND GR.projects_id=P.id AND GR.grant_allocation_id= GA.id AND GR.grant_allocation_split_id=GS.id AND GS.account_head_id=AH.id AND AHS.id=AH.id GROUP BY AH.id)AS FundsReceived,
	(SELECT SUM(GE.expense_amount) FROM grant_expense GE,grant_allocation GL,grant_allocation_split GAS,account_heads AHS WHERE GE.grant_allocation_split_id=GAS.id AND GL.projects_id=GE.projects_id AND GE.grant_allocation_id=GL.id AND GE.projects_id=P.id AND GAS.account_head_id=AHS.id AND AHS.id=AH.id GROUP BY AH.id) AS Expendeture,
	YEAR(CURDATE()) AS FinancialYear
FROM 
	projects P,
	account_heads AH,
	grant_allocation_split GS,
	grant_expense GES,
	grant_receipt GR
WHERE 
	P.id=$P{projectID}
	AND YEAR(GES.date_of_expense) = STR_TO_DATE($P{reportDate},'%d/%M/%Y')
	AND GS.account_head_id = AH.id
	AND GES.grant_allocation_split_id = GS.id
	-- AND GR.grant_allocation_split_id = GES.grant_allocation_split_id
	AND GS.projects_id = P.id
	
GROUP BY AH.id]]></queryString>

