	<subDataset name="Monthwisedataset" >

		<queryString><![CDATA[SELECT 
	P.name AS ProjectTitle,
	P.code AS IDNo,
	P.principal_investigator_name AS PI,
	CONCAT(YEAR(P.project_start_date)," - ",YEAR(P.project_end_date)) AS ProjectPeriod,
	DATE_FORMAT(DATE(GE.date_of_expense),'%d/%m/%Y') AS DATE,
	MONTHNAME(GE.date_of_expense) AS MONTH,
	AH.name AS SanctionedHead,
	GS.amount AS FundsReceived,
	GE.expense_amount AS Expendeture,
	(SELECT 
		SUM(total) 
	FROM (SELECT 
		SUM(GS.amount) AS total
	FROM 
		grant_allocation_split GS 
	GROUP BY 
		GS.account_head_id) AS TotalFundsReceived) AS TotalFundsReceived,
	YEAR(CURDATE()) AS FinancialYear

FROM 
	projects P,
	account_heads AH,
	grant_allocation_split GS,
	grant_expense GE,
	grant_receipt GR
WHERE 
	P.id=1
	AND MONTHNAME(GE.date_of_expense) = '2009-11-11'
	AND GS.account_head_id = AH.id
	AND GE.grant_allocation_split_id = GS.id
	AND GR.grant_allocation_split_id = GE.grant_allocation_split_id
	AND GS.projects_id = P.id
	
GROUP BY GE.id,AH.id]]></queryString>

