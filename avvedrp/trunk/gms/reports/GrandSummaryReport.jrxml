	<queryString><![CDATA[SELECT ga.id,ga.amount_allocated,p.code ProjectCode ,p.name ProjectName,pa.code Instituion,
pa1.code GrandAgency,

(SELECT  sum(gr1.amount) FROM grant_allocation ga1 LEFT JOIN grant_receipt gr1
ON (  gr1.grant_allocation_id=ga1.id  ) WHERE ga1.id=ga.id
group by ga1.id) as FuntAmt,

(SELECT  sum(gr2.expense_amount) 
FROM grant_allocation ga2
LEFT JOIN grant_expense gr2
 ON  (  gr2.grant_allocation_id=ga2.id  )
WHERE ga2.id=ga.id
group by ga2.id) as FuntExp

FROM grant_allocation ga,
projects p ,party pa ,party pa1
WHERE 
 p.id=ga.projects_id AND
pa.id=ga.party_id AND pa1.id=ga.granter_id  
AND pa1.id =$P{partyID}
order by pa.id]]></queryString>

