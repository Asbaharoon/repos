	<parameter name="Path" isForPrompting="false" class="java.lang.String"/>
	<parameter name="projectID" isForPrompting="false" class="java.lang.String"/>
	<queryString><![CDATA[SELECT 
p.name AS ProjectName,
ef.refundable AS Stats,

(SELECT pa.code FROM party pa WHERE id=ga.granter_id) AS ExternalAgency,

(SELECT IF(SUM(GAT.amount_allocated) IS NULL,0,SUM(GAT.amount_allocated)) FROM grant_allocation GAT 
         WHERE GAT.id=ef.grant_allocation_id) AS AmtAllocated,

(SELECT IF(SUM(FT.amount) IS NULL,0,SUM(FT.amount)) FROM grant_allocation GA LEFT JOIN 
         fund_transfer FT  ON(GA.id=FT.grant_allocation_id)
         WHERE GA.id=ga.id AND DATE(FT.date_of_transfer) GROUP BY GA.id) AS FundTransfd,
         
(SELECT  IF(SUM(ge.expense_amount) IS NULL,0,SUM(ge.expense_amount)) 
	FROM grant_allocation GA1
	LEFT JOIN grant_expense ge
	ON  (  ge.grant_allocation_id=GA1.id  )
	WHERE GA1.id=ga.id AND DATE(ge.date_of_expense)  
	GROUP BY GA1.id) AS FuntExp,
	
(SELECT IF(SUM(efr.refund_amount)	IS NULL,0,SUM(efr.refund_amount))
	FROM external_fund_allocation efa
	LEFT JOIN external_fund_refund efr
	ON (efr.external_fund_allocation_id=efa.id )
	WHERE efa.id=ef.id AND DATE(efr.date_of_refund)
	GROUP BY efa.id) AS RefndAmt
	
FROM
projects p,grant_allocation ga,party pa,external_fund_allocation ef

WHERE 
ga.projects_id=p.id
AND ef.grant_allocation_id=ga.id
AND p.id=$P{projectID}
GROUP BY ef.id ORDER BY ef.id;]]></queryString>

