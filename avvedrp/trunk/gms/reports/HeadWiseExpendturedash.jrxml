grp.name grantPeriod
FROM account_heads ah,grant_allocation_split gas left join grant_expense ge on (ge.projects_id=gas.projects_id AND
ge.grant_allocation_split_id=gas.id ),
projects p,grant_period grp  
WHERE ah.id= gas.account_head_id AND

p.id=gas.projects_id AND

grp.id=gas.grant_period_id and
 p.id=$P{projectID}
group by gas.id]]></queryString>

