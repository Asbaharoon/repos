	<parameter name="projectID" isForPrompting="true" class="java.lang.String"/>
	<parameter name="grantPeriod" isForPrompting="true" class="java.lang.String"/>
	<parameter name="Path" isForPrompting="false" class="java.lang.String"/>
	<queryString><![CDATA[SELECT 
	
	DATE_FORMAT(PJ.project_start_date,('%d-%m-%Y')) AS StartDate,
	DATE_FORMAT(DATE(CURDATE()),('%d-%m-%Y')) AS EndDate,
	PJ.name AS ProjectTitle,
	GP.start_date AS fromDate,
	GP.end_date AS toDate,
	(SELECT I.name FROM investigator I,projectspimap PMAP WHERE I.id=PMAP.investigator_id AND PMAP.projects_id=PJ.id AND PMAP.role='PI') AS PI,
	GAG.name_of_the_institution AS SponsoringAgency,
	PY.name_of_the_institution AS Inst,
	GA.sanction_order_no AS SanctionLetterNo,
	GA.date_of_allocation AS SanctionLetterDate,
	(SELECT 
		SUM(total) 
	FROM 
		(SELECT 
			SUM(GA.amount_allocated) AS total
		 FROM 
			grant_allocation GA,
			projects P 
		 WHERE 
			
			GA.projects_id = P.id
			AND (P.parent_id =$P{projectID} OR P.id =$P{projectID})
		 GROUP BY GA.projects_id )AS TotalGrant) AS TotalGrant,
	AH.code AS BudgetHead,
	SUM(GE.expense_amount) AS BudgetAmount,
	(SELECT 
		SUM(total) 
	 FROM
		(SELECT 
			SUM(GR.amount) AS total
		 FROM 
			grant_receipt GR,
			projects P 
		 WHERE 
			
			GR.projects_id = P.id
			AND (P.parent_id =$P{projectID} OR P.id =$P{projectID})
			AND DATE(GR.date_of_receipt) <= DATE(CURDATE())
		GROUP BY 
			GR.projects_id
		)
	AS GrantUpToEndDate)AS GrantUpToEndDate,
	(0.00) AS OpeningBalance,
	(SELECT 
		SUM(total) 
	 FROM
		(SELECT 
			SUM(GR.amount) AS total
		FROM 
			grant_receipt GR,
			grant_allocation_split GS,
			grant_period GP,
			projects P 
		WHERE 
			
			GR.projects_id =  P.id
			AND GR.grant_allocation_split_id = GS.id
			AND GS.grant_period_id = GP.id
			AND GP.id=$P{grantPeriod}
			AND (P.parent_id =$P{projectID} OR P.id =$P{projectID})
		GROUP BY 
			GR.projects_id
	)AS ReceiptsDuringPeriod)AS ReceiptsDuringPeriod,
	(SELECT 
		SUM(total) 
	 FROM
		(SELECT 
			SUM(GE.expense_amount) AS total
		FROM 
			grant_expense GE,
			grant_allocation_split GS,
			grant_period GP,
			projects P 
		WHERE 
			
			GE.projects_id =  P.id
			AND GE.grant_allocation_split_id = GS.id
			AND GS.grant_period_id = GP.id
			AND GP.id=$P{grantPeriod}
			AND (P.parent_id =$P{projectID} OR P.id =$P{projectID})
		GROUP BY 
			GE.projects_id
		)AS TotalExpenditure)AS TotalExpenditure
	
FROM 
	projects PJ,
	party PY ,
	grant_allocation GA LEFT  JOIN party GAG ON (GAG.id = GA.granter_id),
	account_heads AH,
	grant_allocation_split GS,
	grant_expense GE,
	grant_period GP
WHERE
	GA.projects_id = PJ.id
	AND PY.id = GA.party_id
	AND AH.id = GS.account_head_id
	AND GE.projects_id = PJ.id
	AND GE.grant_allocation_split_id =GS.id	
	AND GS.grant_period_id = GP.id
	AND GS.projects_id=PJ.id
	AND GA.id= GS.grant_allocation_id
	AND (PJ.parent_id =$P{projectID} OR PJ.id =$P{projectID})
	AND GP.id=$P{grantPeriod}
GROUP BY 
	GE.grant_allocation_split_id]]></queryString>

