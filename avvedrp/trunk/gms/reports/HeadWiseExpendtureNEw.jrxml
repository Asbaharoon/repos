	<queryString><![CDATA[SELECT ga.id, ah.name  AS AcHead ,gas.amount BudjAmt,IF(SUM(ge.expense_amount) IS NULL,0,SUM(ge.expense_amount)) AS ExpAmt ,p.name AS ProjectName,
pa.name_of_the_institution AS InstName,grp.name grantPeriod,
(SELECT  SUM(grntRcpt1.amount) 
FROM grant_allocation_split grntAllocSplit1 INNER JOIN grant_allocation grntAlloc1
ON(grntAllocSplit1.grant_allocation_id=grntAlloc1.id ) LEFT JOIN grant_receipt grntRcpt1
 ON  (grntRcpt1.grant_allocation_split_id=grntAllocSplit1.id AND  grntRcpt1.grant_allocation_id=grntAlloc1.id  )
WHERE grntAlloc1.id=ga.id
GROUP BY grntAlloc1.id) AS FuntAmt,
(SELECT  SUM(grntRcpt2.amount) 
FROM grant_allocation_split grntAllocSplit2 INNER JOIN grant_allocation grntAlloc2
ON(grntAllocSplit2.grant_allocation_id=grntAlloc2.id ) INNER JOIN grant_receipt grntRcpt2
 ON  (grntRcpt2.grant_allocation_split_id=grntAllocSplit2.id AND  grntRcpt2.grant_allocation_id=grntAlloc2.id  )
WHERE grntAlloc2.id=ga.id
GROUP BY grntAlloc2.id) AS FundRcvd,
(SELECT IF(SUM(amount) IS NULL,0,SUM(amount)) FROM fund_transfer FT INNER JOIN 
          grant_allocation ON(grant_allocation.id=FT.grant_allocation_id) INNER JOIN 
          projects ON(projects.id=grant_allocation.projects_id)
         WHERE projects.parent_id= $P{projectID}) AS FundTrnfd
FROM account_heads ah,grant_allocation_split gas,grant_expense ge,grant_allocation ga,
projects p,party pa ,grant_period grp  
WHERE ah.id= gas.account_head_id AND
gas.grant_allocation_id=ga.id AND
ge.grant_allocation_id=ga.id AND
ge.grant_allocation_split_id=gas.id AND
 p.id=ge.projects_id AND
pa.id=ga.party_id
AND grp.id=gas.grant_period_id
AND gas.grant_period_id=$P{periodID}
AND p.id=$P{projectID}
AND pa.id=$P{partyID}
GROUP BY gas.id]]></queryString>

