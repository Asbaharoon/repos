	<parameter name="projectID" isForPrompting="true" class="java.lang.String"/>
	<parameter name="grantPeriod" isForPrompting="true" class="java.lang.String"/>
	<parameter name="Path" isForPrompting="false" class="java.lang.String"/>
	<parameter name="reportDate" isForPrompting="false" class="java.util.Date"/>
	<parameter name="reportDateTo" isForPrompting="false" class="java.util.Date"/>
	<queryString><![CDATA[SELECT 
    DATE_FORMAT(PJ.project_start_date,('%d-%m-%Y')) AS StartDate,
    DATE_FORMAT(DATE(CURDATE()),('%d-%m-%Y')) AS EndDate,
      
      (SELECT (PT.name_of_the_institution)  FROM   grant_allocation GA,projects P,party PT
    WHERE GA.projects_id=P.id
    AND GA.party_id = PT.id
    AND  P.id =$P{projectID} GROUP BY P.ID) AS Inst,
    
       
    (SELECT P.name 
     FROM   grant_allocation GA,projects P,party PT
    WHERE GA.projects_id=P.id
    AND GA.party_id = PT.id
    AND  P.id =$P{projectID} GROUP BY P.ID  ) AS ProjectTitle,
    
       
    (SELECT  P.code  FROM   grant_allocation GA,projects P,party PT
    WHERE GA.projects_id=P.id
    AND GA.party_id = PT.id
    AND  P.id =$P{projectID} GROUP BY P.ID) AS ProjectCode ,    

    (SELECT I.name FROM investigator I,projectspimap PMAP WHERE I.id=PMAP.investigator_id AND PMAP.projects_id=PJ.id AND PMAP.role='PI' GROUP BY PJ.id) AS PI,
    GAG.name_of_the_institution AS SponsoringAgency,
    GAT.sanction_order_no AS SanctionLetterNo,
    GAT.date_of_allocation AS SanctionLetterDate,
    (SELECT SUM(GA.amount_allocated) FROM grant_allocation GA ,projects P,party PT
    WHERE GA.projects_id=P.id
    AND GA.party_id = PT.id
    AND  P.id =$P{projectID})AS GrantAllocated,
        (SELECT AH.code FROM account_heads AH,grant_allocation_split GS,projects P,utilization un
WHERE 
    GS.account_head_id = AH.id
    AND GS.id=gas.id
    AND (un.projects_id IN (SELECT p.id FROM projects p WHERE p.parent_id=$P{projectID}) OR PJ.id =$P{projectID})
    AND (GS.projects_id = un.projects_id OR GS.projects_id =$P{projectID})
    GROUP BY GS.id)AS BudgetHead,
    (SELECT PA.code FROM account_heads AH,grant_allocation_split GS,projects P,party PA,utilization un
    WHERE 
        GS.account_head_id = AH.id
        AND GS.id=gas.id
        AND GAT.party_id = PA.id
        AND (un.projects_id IN (SELECT p.id FROM projects p WHERE p.parent_id=$P{projectID}) OR PJ.id =$P{projectID})
        AND (GS.projects_id = un.projects_id OR GS.projects_id =$P{projectID})
        GROUP BY GS.id) AS BudgetInstitute,
    (SELECT IF(SUM(GE.expense_amount)IS NULL,0,SUM(GE.expense_amount))
    FROM grant_expense GE,grant_allocation_split GS,account_heads AH,projects P
    WHERE GE.grant_allocation_split_id = GS.id
        AND GS.id=gas.id
        AND GS.account_head_id = AH.id
        AND GE.projects_id = P.id
        AND DATE(GE.date_of_expense) BETWEEN $P{reportDate} AND $P{reportDateTo}
        GROUP BY GE.grant_allocation_split_id) AS BudgetAmount,
    (SELECT SUM(total) 
     FROM
        (SELECT 
            IF(SUM(GR.amount) IS NULL,0,SUM(GR.amount)) AS total
            FROM 
                grant_receipt GR,
                projects P 
            WHERE 
                GR.projects_id = P.id
                AND P.id =$P{projectID}
                AND DATE(GR.date_of_receipt) BETWEEN $P{reportDate} AND $P{reportDateTo}
            GROUP BY GR.projects_id) AS TotalGrant) 
            AS TotalGrant,
    (SELECT 
        SUM(total) 
     FROM
        (SELECT 
            IF(SUM(GR.amount) IS NULL,0,SUM(GR.amount)) AS total
         FROM 
            grant_receipt GR,
            projects P 
         WHERE 
            
            GR.projects_id = P.id
            AND P.id =$P{projectID}
            AND DATE(GR.date_of_receipt) <= $P{reportDateTo}
        GROUP BY 
            GR.projects_id
        )
    AS GrantUpToEndDate)AS GrantUpToEndDate,
    (0.00) AS OpeningBalance,
    
    
    (SELECT 
        SUM(total) 
     FROM
        (SELECT 
            IF(SUM(GR.amount) IS NULL,0,SUM(GR.amount)) AS total
        FROM 
            grant_receipt GR,
            projects P 
        WHERE 
            
            GR.projects_id =  P.id
            AND P.id =$P{projectID}
            AND DATE(GR.date_of_receipt) BETWEEN $P{reportDate} AND $P{reportDateTo}
        GROUP BY 
            GR.projects_id
    )AS ReceiptsDuringPeriod)AS ReceiptsDuringPeriod,
    
    (SELECT 
        SUM(total) 
     FROM
        (SELECT 
            IF(SUM(GR.amount) IS NULL,0,SUM(GR.amount)) AS total
        FROM 
            grant_receipt GR,
            projects P 
        WHERE 
            
            GR.projects_id =  P.id
            AND P.id =$P{projectID}
            AND DATE(GR.date_of_receipt) < $P{reportDate}
        GROUP BY 
            GR.projects_id
    )AS ReceiptsOpeningPeriod)AS ReceiptsOpeningPeriod,
    (SELECT 
        SUM(total) 
     FROM
        (SELECT 
            IF(SUM(GE.expense_amount) IS NULL,0,SUM(GE.expense_amount)) AS total
        FROM 
            grant_expense GE,
            projects P 
        WHERE 
            
            GE.projects_id =  P.id
            AND P.id =$P{projectID}
            AND DATE(GE.date_of_expense) BETWEEN $P{reportDate} AND $P{reportDateTo}
        GROUP BY 
            GE.projects_id
        )AS TotalExpenditure)AS TotalExpenditure,
        
        (SELECT 
        SUM(total) 
     FROM
        (SELECT 
            IF(SUM(GE.expense_amount) IS NULL,0,SUM(GE.expense_amount)) AS total
        FROM 
            grant_expense GE,
            projects P 
        WHERE 
            
            GE.projects_id =  P.id
            AND P.id =$P{projectID}
            AND DATE(GE.date_of_expense) < $P{reportDate} 
        GROUP BY 
            GE.projects_id
        )AS TotalOpenExpenditure)AS TotalOpenExpenditure,
        
        (SELECT 
        SUM(total) 
     FROM
        (SELECT 
            IF(SUM(GE.expense_amount) IS NULL,0,SUM(GE.expense_amount)) AS total
        FROM 
            grant_expense GE,
            projects P 
        WHERE 
            
            GE.projects_id =  P.id
            AND (P.parent_id =$P{projectID} OR P.id =$P{projectID})
            AND DATE(GE.date_of_expense) BETWEEN $P{reportDate} AND $P{reportDateTo} 
        GROUP BY 
            GE.projects_id
        )AS TotalExpenditure)AS TotalExpense,
        
        (SELECT IF(SUM(amount) IS NULL,0,SUM(amount)) FROM fund_transfer FT INNER JOIN 
          grant_allocation ON(grant_allocation.id=FT.grant_allocation_id) INNER JOIN 
          projects ON(projects.id=grant_allocation.projects_id)
         WHERE projects.parent_id= $P{projectID} AND FT.date_of_transfer BETWEEN $P{reportDate} AND $P{reportDateTo}
     ) AS FundAmt,
     
     (SELECT IF(SUM(amount) IS NULL,0,SUM(amount)) FROM fund_transfer FT INNER JOIN 
          grant_allocation ON(grant_allocation.id=FT.grant_allocation_id) INNER JOIN 
          projects ON(projects.id=grant_allocation.projects_id)
         WHERE projects.parent_id= $P{projectID} AND FT.date_of_transfer < $P{reportDate}  
     ) AS FundOpenAmt
    
FROM 
    projects PJ,
    party PY ,
    grant_allocation_split gas,
    utilization un,
    grant_allocation GAT LEFT  JOIN party GAG ON (GAG.id = GAT.granter_id)
    
WHERE
           
        GAT.projects_id = PJ.id
    AND PY.id = GAT.party_id
    AND gas.grant_allocation_id =GAT.id
    AND (un.projects_id IN (SELECT p.id FROM projects p WHERE p.parent_id=$P{projectID}) OR PJ.id =$P{projectID})
    AND (PJ.id =un.projects_id OR PJ.id =$P{projectID})
GROUP BY 
    gas.id]]></queryString>

