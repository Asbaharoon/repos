	<parameter name="partyID" isForPrompting="false" class="java.lang.String"/>
	<parameter name="periodID" isForPrompting="false" class="java.lang.String"/>
	<parameter name="reportDate" isForPrompting="false" class="java.lang.String"/>
	<parameter name="reportDateTo" isForPrompting="false" class="java.lang.String"/>
	<queryString><![CDATA[SELECT p.id,gat.id,gat.amount_allocated,p.code ProjectCode ,p.name ProjectName,pa.code Instituion,
pa1.code GrandAgency,

(SELECT  IF(SUM(gr1.amount) IS NULL,0,SUM(gr1.amount)) FROM grant_allocation ga1 LEFT JOIN grant_receipt gr1
ON (  gr1.grant_allocation_id=ga1.id  ) WHERE ga1.id=gat.id AND DATE(gr1.date_of_receipt) BETWEEN STR_TO_DATE($P{reportDate},'%d/%M/%Y') AND STR_TO_DATE($P{reportDateTo},'%d/%M/%Y') 
GROUP BY ga1.id) AS FuntAmt,
(SELECT IF(SUM(FT.amount) IS NULL,0,SUM(FT.amount)) FROM grant_allocation GA LEFT JOIN 
         fund_transfer FT  ON(GA.id=FT.grant_allocation_id)
         WHERE GA.id=gat.id AND DATE(FT.date_of_transfer) BETWEEN STR_TO_DATE($P{reportDate},'%d/%M/%Y') AND STR_TO_DATE($P{reportDateTo},'%d/%M/%Y') GROUP BY GA.id) AS FundTransfd,

(SELECT  IF(SUM(gr2.expense_amount) IS NULL,0,SUM(gr2.expense_amount)) 
FROM grant_allocation ga2
LEFT JOIN grant_expense gr2
 ON  (  gr2.grant_allocation_id=ga2.id  )
WHERE ga2.id=gat.id AND DATE(gr2.date_of_expense) BETWEEN STR_TO_DATE($P{reportDate},'%d/%M/%Y') AND STR_TO_DATE($P{reportDateTo},'%d/%M/%Y') 
GROUP BY ga2.id) AS FuntExp

FROM grant_allocation gat,
projects p ,party pa ,party pa1
WHERE 
 p.id=gat.projects_id AND p.active_yes_no='Y'
AND pa.id=gat.party_id AND pa1.id=gat.granter_id  
AND pa1.id =$P{partyID}
ORDER BY pa.id]]></queryString>

