	<queryString><![CDATA[select pr.name as ProjectName,pr.code as ProjectCode,pag.code as Granter,pa.code as Institution,
(SELECT I.name FROM investigator I WHERE I.id=pr.principal_investigator_name_id) as ProjectMgr,ga.amount_allocated as AmtAllocated,

(select  sum(grFR.amount)  from grant_allocation gaFR
left join grant_receipt grFR on  (  grFR.grant_allocation_id=gaFR.id  )
where gaFR.party_id=pa.id and gaFR.projects_id=pr.id
group by gaFR.party_id) as FundRcvd,

(select  sum(geFE.expense_amount)  from grant_allocation gaFE
left join grant_expense geFE on (geFE.grant_allocation_id=gaFE.id  )
where gaFE.party_id=pa.id and gaFE.projects_id=pr.id
group by gaFE.party_id) as FundExp

from projects pr,party pa,grant_allocation ga 
left join party pag on (pag.id = ga.granter_id)
where pr.id = ga.projects_id and pa.id = ga.party_id 
and pa.id = $P{partyID} order by pr.name]]></queryString>

