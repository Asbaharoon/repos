	<parameter name="partyID" isForPrompting="true" class="java.lang.String"/>
	<parameter name="projectType" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT 
      P.name AS ProjectName,
      (SELECT I.name FROM investigator I,projectspimap PMAP WHERE I.id=PMAP.investigator_id AND PMAP.projects_id=P.id AND PMAP.role='PI') AS Investigator,
      PD.name AS DepartmentName,
      (SELECT PY.name_of_the_institution FROM party PY,grant_allocation G WHERE PY.id=G.granter_id AND G.projects_id=GA.projects_id AND P.id=G.projects_id GROUP BY G.projects_id)AS Grantor,
      (SELECT SUM(GR.amount)FROM grant_receipt GR,grant_allocation G WHERE GR.projects_id=G.projects_id AND GR.projects_id=P.id AND GR.grant_allocation_id=G.id GROUP BY GR.projects_id)AS ReceivedAmount,
      (SELECT SUM(GR.amount)
      FROM grant_receipt GR,grant_allocation G,grant_allocation_split GS,account_heads AH 
      WHERE GR.projects_id=G.projects_id AND GR.projects_id=P.id AND GR.grant_allocation_split_id=GS.id AND GS.account_head_id=AH.id AND AH.code='OVERHEADS' AND GR.grant_allocation_id=G.id 
      GROUP BY GR.projects_id)AS OverHeads,
      (SELECT SUM(GE.expense_amount)FROM grant_expense GE,grant_allocation G WHERE GE.projects_id=G.projects_id AND GE.projects_id=P.id AND GE.grant_allocation_id=G.id GROUP BY GE.projects_id)AS Expenditure,
      P.code
      
FROM 
	projects P,
	party PT,
	party_department PD,
	project_department_map PM,
	grant_allocation GA,
	project_type PJ
WHERE 
	P.id=PM.projects_id
	AND PD.id= PM.party_department_id
	AND PT.id=$P{partyID}
	AND GA.party_id=PT.id
	AND GA.projects_id=P.id
	AND P.project_type_id=PJ.id
	AND PJ.type=$P{projectType}
	
GROUP BY 
	PM.projects_id
ORDER BY 
	PD.name]]></queryString>

