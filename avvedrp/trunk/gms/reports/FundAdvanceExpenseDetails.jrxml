	<parameter name="projectID" isForPrompting="false" class="java.lang.String"/>
	<parameter name="Path" isForPrompting="false" class="java.lang.String"/>
	<parameter name="reportDate" isForPrompting="false" class="java.lang.String"/>
	<parameter name="reportDateTo" isForPrompting="false" class="java.lang.String"/>
	<queryString><![CDATA[SELECT 
p.name AS ProjectName,
fa.advance_description AS AdvanceDes,
fa.advance_amount AS AdvanceAmt,
fa.status AS stats,
ge.expense_amount AS ExpAmt,
ah.name AS AccName,
ge.date_of_expense AS DateOfExp,
ge.description AS ExpDes,

(SELECT IF (STATUS IS NULL,(fac.advance_amount - SUM(gte.expense_amount)),0)
FROM
fund_advance fac LEFT JOIN grant_expense gte ON (fac.fund_advance_code=gte.fund_advance_code)
WHERE
fac.fund_advance_code = fa.fund_advance_code
GROUP BY fac.fund_advance_code ORDER BY fac.id) AS Balance,

(SELECT IF (STATUS IS NULL,0,(fac.advance_amount - SUM(gte.expense_amount)))
FROM
fund_advance fac LEFT JOIN grant_expense gte ON (fac.fund_advance_code=gte.fund_advance_code)
WHERE
fac.fund_advance_code = fa.fund_advance_code
GROUP BY fac.fund_advance_code ORDER BY fac.id) AS RefundedAmt

FROM
projects p,grant_allocation ga,grant_allocation_split gas,account_heads ah,
fund_advance fa LEFT JOIN grant_expense ge ON (fa.fund_advance_code=ge.fund_advance_code)

WHERE 
ga.projects_id=p.id
AND fa.fund_advance_code=ge.fund_advance_code
AND ah.id=gas.account_head_id
AND gas.id=ge.grant_allocation_split_id
AND ge.projects_id=p.id
AND fa.grant_allocation_id=ga.id
AND DATE(ge.date_of_expense) BETWEEN STR_TO_DATE($P{reportDate},'%d/%M/%Y') AND STR_TO_DATE($P{reportDateTo},'%d/%M/%Y')
AND p.id=$P{projectID}
ORDER BY fa.id;]]></queryString>

