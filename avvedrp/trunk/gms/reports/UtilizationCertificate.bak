	<parameter name="projectID" isForPrompting="true" class="java.lang.String"/>
	<parameter name="grantPeriod" isForPrompting="true" class="java.lang.String"/>
	<parameter name="Path" isForPrompting="false" class="java.lang.String"/>
	<queryString><![CDATA[SELECT 
	PJ.name AS ProjectName,
	CONCAT(YEAR(GP.start_date),'-',YEAR(GP.end_date)) AS financialYear,
	CONCAT(YEAR(GP.start_date)+1,'-',YEAR(GP.end_date)+1) AS nextFinancialYear,
	YEAR(GP.end_date) AS currentYear,
	GR.reference_id AS LetterNo,
	(SELECT 
		SUM(total)
	 FROM 
		(SELECT 
			SUM(GR.amount) AS total
		FROM 
			grant_receipt GR,
			grant_allocation_split GS,
			grant_period GP,
			projects P 
		WHERE 
			
			GR.projects_id =  P.id
			AND GR.grant_allocation_split_id = GS.id
			AND GS.grant_period_id = GP.id
			AND GP.id=$P{grantPeriod}
			AND (P.parent_id =$P{projectID} OR P.id =$P{projectID})
		GROUP BY 
			GR.projects_id)AS Amount)AS Amount,
	PY.name_of_the_institution AS Institution,
	(SELECT 
		SUM(total) 
	 FROM
			(SELECT 
				SUM(GE.expense_amount) AS total
			FROM 
				grant_expense GE,
				grant_allocation_split GS,
				grant_period GP,
				projects P 
			WHERE 
				
				GE.projects_id =  P.id
				AND GE.grant_allocation_split_id = GS.id
				AND GS.grant_period_id = GP.id
				AND GP.id=$P{grantPeriod}
				AND (P.parent_id =$P{projectID} OR P.id =$P{projectID})
			GROUP BY 
				GE.projects_id
			 ) AS ExpenseAmount
	 ) AS ExpenseAmount
FROM 
	projects PJ,
	grant_receipt GR,
	party PY,
	grant_expense GE,
	grant_period GP,
	grant_allocation_split GS,
	grant_allocation GA
WHERE 
	GA.projects_id  = PJ.id
	AND GR.projects_id = GA.projects_id 
	AND PY.id = GA.party_id
	AND GR.projects_id = GE.projects_id
	AND GE.grant_allocation_split_id = GS.id
	AND GS.grant_period_id = GP.id
	AND GP.id=$P{grantPeriod}
	AND (PJ.parent_id =$P{projectID} OR PJ.id =$P{projectID})
GROUP BY 
	PJ.id,GE.projects_id]]></queryString>

