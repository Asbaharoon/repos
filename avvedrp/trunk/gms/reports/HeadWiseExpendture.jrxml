	<parameter name="reportDateTo" isForPrompting="false" class="java.lang.String"/>
	<queryString><![CDATA[SELECT  ah.name  AS AcHead ,
gs.amount BudjAmt,

(SELECT IF(SUM(ge.expense_amount) IS NULL,0,SUM(ge.expense_amount))  FROM grant_allocation_split gas 
LEFT JOIN grant_expense ge ON (ge.projects_id=gas.projects_id 
AND ge.grant_allocation_split_id=gas.id)
WHERE  gas.id = gs.id AND DATE(ge.date_of_expense) BETWEEN STR_TO_DATE($P{reportDate},'%d/%M/%Y') AND STR_TO_DATE($P{reportDateTo},'%d/%M/%Y'))AS ExpAmt ,
p.name AS ProjectName,
(SELECT  IF(SUM(gr1.amount) IS NULL,0,SUM(gr1.amount)) FROM grant_allocation ga1 LEFT JOIN grant_receipt gr1
ON (  gr1.grant_allocation_id=ga1.id  ) WHERE ga1.id=ga.id AND DATE(gr1.date_of_receipt) BETWEEN STR_TO_DATE($P{reportDate},'%d/%M/%Y') AND STR_TO_DATE($P{reportDateTo},'%d/%M/%Y') 
GROUP BY ga1.id) AS RcdAmt,
(SELECT IF(SUM(amount) IS NULL,0,SUM(amount)) FROM fund_transfer FT 
INNER JOIN grant_allocation ON(grant_allocation.id=FT.grant_allocation_id) 
INNER JOIN projects ON(projects.id=grant_allocation.projects_id)
WHERE projects.parent_id= $P{projectID}
AND DATE(FT.date_of_transfer) BETWEEN STR_TO_DATE($P{reportDate},'%d/%M/%Y') AND STR_TO_DATE($P{reportDateTo},'%d/%M/%Y')
) AS FundAmt

FROM 
projects p,grant_allocation ga,account_heads ah,grant_allocation_split gs 
 
WHERE ah.id= gs.account_head_id 
AND p.id=gs.projects_id 
AND gs.grant_allocation_id=ga.id 
AND p.id=$P{projectID}
GROUP BY gs.id]]></queryString>

