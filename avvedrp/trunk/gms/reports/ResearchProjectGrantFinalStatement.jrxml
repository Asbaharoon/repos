	<parameter name="projectID" isForPrompting="true" class="java.lang.String"/>
	<parameter name="reportDate" isForPrompting="true" class="java.lang.String"/>
	<parameter name="reportDateTo" isForPrompting="true" class="java.lang.String"/>
	<parameter name="partyID" isForPrompting="true" class="java.lang.String"/>
	<parameter name="Path" isForPrompting="false" class="java.lang.String"/>
	<queryString><![CDATA[SELECT 
	P.name AS ProjectName,
	GE.expense_amount AS ExpenceAmount,
	GE.date_of_expense,

	AH.name AS AccountHead,
	(SELECT name_of_the_institution FROM party PT,grant_allocation GL WHERE GL.granter_id=PT.id AND GL.id=GA.id) AS FundingAgency,
	(SELECT SUM(GR.amount) 
	FROM grant_receipt GR,grant_allocation GL,grant_allocation_split GAS,account_heads AHS 
	WHERE GR.grant_allocation_split_id=GAS.id AND GR.grant_allocation_id=GL.id AND GR.projects_id=P.id AND AHS.id=GAS.account_head_id AND GAS.id=GS.id AND DATE(GR.date_of_receipt) BETWEEN STR_TO_DATE($P{reportDate},'%d/%M/%Y') AND STR_TO_DATE($P{reportDateTo},'%d/%M/%Y') GROUP BY GAS.id) AS Amount,
	(SELECT SUM(GSE.expense_amount) FROM grant_expense GSE WHERE GSE.projects_id=P.id GROUP BY GSE.projects_id) AS ToatalCost,
	(SELECT SUM(GR.amount) 
	FROM grant_receipt GR,grant_allocation GL,grant_allocation_split GAS,account_heads AHS 
	WHERE GR.grant_allocation_split_id=GAS.id AND GR.grant_allocation_id=GL.id AND GR.projects_id=P.id AND AHS.id=GAS.account_head_id AND AHS.code!='OVERHEADS' AND DATE(GR.date_of_receipt) BETWEEN STR_TO_DATE($P{reportDate},'%d/%M/%Y') AND STR_TO_DATE($P{reportDateTo},'%d/%M/%Y') GROUP BY GR.projects_id) AS TotRctAmnt,
	GA.sanction_order_no AS SanctionNo,
	GA.date_of_allocation AS DateofAllocation,
	P.project_start_date AS project_start_date,
	P.project_end_date AS project_end_date 
	
FROM
	projects P,
	party PY,
	grant_allocation GA,
	grant_allocation_split GS,
	grant_receipt GR,
	account_heads AH,
	grant_expense GE
WHERE 
	P.id=$P{projectID} AND
	PY.id=$P{partyID} AND
	GA.projects_id=P.id AND
	GA.party_id=PY.id AND
	GA.id=GE.grant_allocation_id AND
	GE.projects_id=P.id AND
	GE.grant_allocation_split_id=GS.id AND
	GS.projects_id=P.id AND
	GA.id=GS.grant_allocation_id AND
	GS.account_head_id=AH.id AND
	DATE(GE.date_of_expense) BETWEEN STR_TO_DATE($P{reportDate},'%d/%M/%Y') AND STR_TO_DATE($P{reportDateTo},'%d/%M/%Y')
	
GROUP BY GE.id]]></queryString>

