	<queryString><![CDATA[SELECT ga.id,pr.name AS ProjectName,pr.code AS ProjectCode,pr.project_start_date AS StartDate,pr.project_end_date AS EndDate,pag.code AS Granter,pa.code AS Institution,
SUM(ga.amount_allocated) AS AmtAllocated,
(SELECT I.name FROM investigator I,projectspimap PMAP WHERE I.id=PMAP.investigator_id AND PMAP.projects_id=pr.id AND PMAP.role='PI' GROUP BY pr.id) AS ProjectMgr,
(SELECT IF(SUM(amount) IS NULL,0,SUM(amount)) FROM fund_transfer FT INNER JOIN 
          grant_allocation ON(grant_allocation.id=FT.grant_allocation_id) INNER JOIN 
          projects ON(projects.id=grant_allocation.projects_id)
         WHERE projects.parent_id=pr.id AND DATE(FT.date_of_transfer) GROUP BY pr.id) AS FundAmt,
         
(SELECT  IF(SUM(grFR.amount) IS NULL,0,SUM(grFR.amount))  FROM projects prRe
LEFT JOIN grant_receipt grFR ON (grFR.projects_id=prRe.id)
WHERE prRe.id=pr.id )AS FundRcvd,


(SELECT  IF(SUM(geFE.expense_amount) IS NULL,0,SUM(geFE.expense_amount))  FROM projects prRe
LEFT JOIN grant_expense geFE ON  (geFE.projects_id=prRe.id)
WHERE prRe.id=pr.id ) AS FundExp

FROM projects pr,party pa,grant_allocation ga 
LEFT JOIN party pag ON (pag.id = ga.granter_id)
WHERE pr.id = ga.projects_id AND pa.id = ga.party_id 
AND pa.id = $P{PartyID} GROUP BY pr.id ORDER BY pr.name]]></queryString>

