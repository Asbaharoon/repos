	<queryString><![CDATA[SELECT 
P.name AS projectName,
PE.emp_name AS employeeName,
ED.designation AS currentDesignation,
PEE.designation AS previousDesignation,
PEE.joining_date AS joiningDate,
PEE.relieving_date AS relievingDate,
PEE.organization_name AS organization

FROM
projects P,
project_employee PE,
employee_designation ED,
project_employee_experience PEE

WHERE
PEE.project_employee_id = PE.id
AND PE.employee_designation_id = ED.id
AND PE.projects_id = P.id
AND P.id = $P{projectID}
AND PEE.status = 'Y'
GROUP BY PEE.id]]></queryString>

