	<parameter name="partyID" isForPrompting="false" class="java.lang.String"/>
	<parameter name="periodID" isForPrompting="false" class="java.lang.String"/>
	<parameter name="reportDate" isForPrompting="false" class="java.lang.String"/>
	<parameter name="reportDateTo" isForPrompting="false" class="java.lang.String"/>
	<queryString><![CDATA[SELECT pr.name AS ProjectName,pr.code AS ProjectCode,pag.code AS Granter,pa.code AS Institution,
SUM(ga.amount_allocated) AS AmtAllocated,
(SELECT I.name FROM investigator I,projectspimap PMAP WHERE I.id=PMAP.investigator_id AND PMAP.projects_id=pr.id AND PMAP.role='PI' GROUP BY pr.id) AS ProjectMgr,
(SELECT IF(SUM(amount) IS NULL,0,SUM(amount)) FROM fund_transfer FT INNER JOIN 
          grant_allocation ON(grant_allocation.id=FT.grant_allocation_id) INNER JOIN 
          projects ON(projects.id=grant_allocation.projects_id)
         WHERE projects.parent_id=pr.id AND DATE(FT.date_of_transfer) BETWEEN STR_TO_DATE($P{reportDate},'%d/%M/%Y') AND STR_TO_DATE($P{reportDateTo},'%d/%M/%Y') 
         GROUP BY pr.id) AS FundAmt,

(SELECT  IF(SUM(grFR.amount) IS NULL,0,SUM(grFR.amount))  FROM grant_allocation gaFR
LEFT JOIN grant_receipt grFR ON (grFR.grant_allocation_id=gaFR.id)
WHERE gaFR.id=ga.id AND DATE (grFR.date_of_receipt) BETWEEN STR_TO_DATE($P{reportDate},'%d/%M/%Y') AND STR_TO_DATE($P{reportDateTo},'%d/%M/%Y')  GROUP BY gaFR.id) AS FundRcvd,

(SELECT  IF(SUM(geFE.expense_amount) IS NULL,0,SUM(geFE.expense_amount))  FROM grant_allocation gaFE
LEFT JOIN grant_expense geFE ON (geFE.grant_allocation_id=gaFE.id)
WHERE gaFE.id=ga.id AND DATE (geFE.date_of_expense) BETWEEN STR_TO_DATE($P{reportDate},'%d/%M/%Y') AND STR_TO_DATE($P{reportDateTo},'%d/%M/%Y')  GROUP BY gaFE.id) AS FundExp

FROM projects pr,party pa,grant_allocation ga 
LEFT JOIN party pag ON (pag.id = ga.granter_id)
WHERE pr.id = ga.projects_id AND pa.id = ga.party_id 
AND pa.id = $P{partyID} GROUP BY pr.id ORDER BY pr.name]]></queryString>

