	<parameter name="reportDate" isForPrompting="true" class="java.lang.String"/>
	<parameter name="partyID" isForPrompting="true" class="java.lang.String"/>
	<parameter name="Path" isForPrompting="false" class="java.lang.String"/>
	<queryString><![CDATA[SELECT 
      P.name AS ProjectTitle,
      (SELECT I.name FROM investigator I,projectspimap PMAP WHERE I.id=PMAP.investigator_id AND PMAP.projects_id=P.id AND PMAP.role='PI' GROUP BY P.id) AS PI,
      PD.department_code AS Department,
      PRT.type AS ProjectType,
      (SELECT PY.name_of_the_institution FROM party PY,grant_allocation G WHERE PY.id=G.granter_id AND G.projects_id=GA.projects_id AND P.id=G.projects_id GROUP BY G.projects_id)AS FundingAgency,
      (SELECT SUM(GR.amount)FROM grant_receipt GR,grant_allocation G WHERE GR.projects_id=G.projects_id AND GR.projects_id=P.id AND GR.grant_allocation_id=G.id AND DATE(GR.date_of_receipt)<=STR_TO_DATE($P{reportDate},'%d/%M/%Y') GROUP BY GR.projects_id)AS FundsReceived,
      (SELECT SUM(GR.amount)
      FROM grant_receipt GR,grant_allocation G,grant_allocation_split GS,account_heads AH 
      WHERE GR.projects_id=G.projects_id AND GR.projects_id=P.id AND GR.grant_allocation_split_id=GS.id AND GS.account_head_id=AH.id AND AH.code='OVERHEADS' AND GR.grant_allocation_id=G.id AND DATE(GR.date_of_receipt)<=STR_TO_DATE($P{reportDate},'%d/%M/%Y')
      GROUP BY GR.projects_id)AS OverHeads,
      (SELECT SUM(GE.expense_amount)FROM grant_expense GE,grant_allocation G WHERE GE.projects_id=G.projects_id AND GE.projects_id=P.id AND GE.grant_allocation_id=G.id AND DATE(GE.date_of_expense)<=STR_TO_DATE($P{reportDate},'%d/%M/%Y') GROUP BY GE.projects_id)AS Expenditure
      
FROM 
	projects P,
	party PT,
	party_department PD,
	project_department_map PM,
	grant_allocation GA,
        project_type PRT
WHERE 
	P.id=PM.projects_id
	AND PD.id= PM.party_department_id
	AND PT.id=$P{partyID}
	AND GA.party_id=PT.id
	AND GA.projects_id=P.id
        AND P.project_type_id=PRT.id
	
GROUP BY PM.projects_id]]></queryString>

