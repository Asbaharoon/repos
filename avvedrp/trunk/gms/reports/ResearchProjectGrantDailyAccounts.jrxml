	<parameter name="reportDate" isForPrompting="true" class="java.lang.String"/>
	<parameter name="projectID" isForPrompting="true" class="java.lang.String"/>
	<parameter name="reportDateTo" isForPrompting="true" class="java.lang.String"/>
	<parameter name="Path" isForPrompting="false" class="java.lang.String"/>
	<queryString><![CDATA[SELECT 
	P.name AS ProjectTitle,
	P.code AS IDNo,
	
	(SELECT I.name FROM investigator I,projectspimap PMAP WHERE I.id=PMAP.investigator_id AND PMAP.projects_id=P.id AND PMAP.role='PI' GROUP BY P.id) AS PI,
	CONCAT(YEAR(P.project_start_date)," - ",YEAR(P.project_end_date)) AS ProjectPeriod,
	DATE_FORMAT(DATE(GE.date_of_expense),'%d/%m/%Y') AS DATE,
	PD.department_code AS FundingDepartment,
	AH.name AS SanctionedHead,
	SUM(GR.amount) AS FundsReceived,
	GE.expense_amount AS Expendeture,
	GA.sanction_order_no AS Sancno,
	GR.reference_id AS Recno,
		(SELECT 
		SUM(total) 
	FROM (SELECT 
		grant_receipt.amount AS total
	FROM 
		projects ,
		grant_allocation_split,grant_expense,grant_receipt,account_heads,grant_allocation
	WHERE   
	        projects.id=$P{projectID} 
		AND DATE(grant_expense.date_of_expense) BETWEEN STR_TO_DATE($P{reportDate},'%d/%M/%Y') AND STR_TO_DATE($P{reportDateTo},'%d/%M/%Y')
		AND grant_allocation_split.account_head_id = account_heads.id
		AND  grant_expense.grant_allocation_split_id = grant_allocation_split.id
		 AND grant_receipt.grant_allocation_split_id = grant_allocation_split.id
		 AND grant_receipt.grant_allocation_id=grant_allocation.id
		 AND grant_allocation.projects_id=projects.id
		 AND grant_receipt.grant_allocation_split_id = grant_expense.grant_allocation_split_id
		AND grant_allocation_split.projects_id = projects.id
	GROUP BY 
		grant_receipt.id) AS TotalFundsReceived) AS TotalFundsReceived,
	YEAR(CURDATE()) AS FinancialYear


FROM 
	projects P,
	account_heads AH,
	grant_allocation_split GS,
	grant_expense GE,
	grant_receipt GR,
	grant_allocation GA,
	party_department PD,
	project_department_map PM
WHERE 
	P.id=$P{projectID}
	AND DATE(GE.date_of_expense) BETWEEN STR_TO_DATE($P{reportDate},'%d/%M/%Y') AND STR_TO_DATE($P{reportDateTo},'%d/%M/%Y')
	AND GS.account_head_id = AH.id
	AND GE.grant_allocation_split_id = GS.id
	AND GR.grant_allocation_split_id=GS.id
	AND GR.grant_allocation_split_id = GE.grant_allocation_split_id
	AND GS.projects_id = P.id
	AND GA.id=GE.grant_allocation_id
	AND GA.id=GR.grant_allocation_id
	AND GA.projects_id=P.id
	AND PM.party_department_id=PD.id
	AND PM.projects_id=$P{projectID}
	
GROUP BY GE.id]]></queryString>

