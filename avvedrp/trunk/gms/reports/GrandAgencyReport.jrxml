	<queryString><![CDATA[SELECT pa1.id as id,count(ga1.id) as number,p.id as projectID,sum(ga1.amount_allocated) as sumAmt,
pa1.code Instituion,pa2.code as grantAgency,

(SELECT  sum(gr.amount) 
FROM grant_allocation ga
LEFT JOIN grant_receipt gr
 ON  (  gr.grant_allocation_id=ga.id  )
WHERE ga.party_id=pa1.id and ga.projects_id=p.id
group by ga.party_id)as FuntAmt

FROM grant_allocation ga1,
projects p ,party pa1 ,party pa2 
WHERE 
p.id=ga1.projects_id AND
pa1.id=ga1.party_id AND pa2.id=ga1.granter_id 
AND pa2.id =$P{partyID}
group by pa1.id]]></queryString>

