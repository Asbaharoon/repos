	<parameter name="projectID" isForPrompting="true" class="java.lang.String"/>
	<parameter name="partyID" isForPrompting="true" class="java.lang.String"/>
	<parameter name="reportDate" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT 
	P.name AS ProjectTitle,
	PT.name_of_the_institution AS FundingAgency,
	P.principal_investigator_name AS PI,
	AH.name AS SanctionedHead,
	GS.amount AS FundsReceived,
	GE.expense_amount AS Expendeture,
	YEAR(STR_TO_DATE($P{reportDate},'%d/%M/%Y')) AS Period,
	GA.sanction_order_no AS Sancno,
	(SELECT 
	SUM(total) 
	FROM (SELECT 
		grant_receipt.amount AS total
	FROM 
		projects,
		account_heads,
		grant_allocation_split,
		grant_expense ,
		grant_receipt ,
		party,
		grant_allocation 
	WHERE 
		grant_allocation_split.account_head_id = account_heads.id
		AND grant_expense.grant_allocation_split_id = grant_allocation_split.id
		AND grant_receipt.grant_allocation_split_id = grant_expense.grant_allocation_split_id
		AND grant_allocation_split.projects_id = projects.id
		AND grant_allocation_split.projects_id=grant_allocation.projects_id
		AND grant_allocation.party_id = party.id
		AND YEAR(grant_receipt.date_of_receipt) = YEAR(STR_TO_DATE($P{reportDate},'%d/%M/%Y'))

	GROUP BY 
		grant_receipt.id) AS TotalFundsReceived) AS TotalFundsReceived,	
	YEAR(CURDATE()) AS FinancialYear

FROM 
	projects P,
	account_heads AH,
	grant_allocation_split GS,
	grant_expense GE,
	grant_receipt GR,
	party PT,
	grant_allocation GA
WHERE 
	P.id=$P{projectID} 
	AND PT.id = $P{partyID}
	AND YEAR(GR.date_of_receipt) = YEAR(STR_TO_DATE($P{reportDate},'%d/%M/%Y'))
	AND GS.account_head_id = AH.id
	AND GE.grant_allocation_split_id = GS.id
	-- AND GR.grant_allocation_split_id = GE.grant_allocation_split_id
	AND GS.projects_id = P.id
	AND GS.projects_id=GA.projects_id
	AND GA.party_id = PT.id
	AND GA.id=GE.grant_allocation_id
	-- AND GA.id=GS.grant_allocation_id
	
GROUP BY GE.id,GS.account_head_id]]></queryString>

