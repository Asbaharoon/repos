	<parameter name="projectID" isForPrompting="true" class="java.lang.String"/>
	<parameter name="partyID" isForPrompting="true" class="java.lang.String"/>
	<parameter name="reportDate" isForPrompting="true" class="java.lang.String"/>
	<parameter name="Path" isForPrompting="false" class="java.lang.String"/>
	<queryString><![CDATA[SELECT 
	P.name AS ProjectTitle,
	(SELECT PY.name_of_the_institution FROM party PY,grant_allocation G WHERE PY.id=G.granter_id AND G.projects_id=GA.projects_id AND P.id=G.projects_id GROUP BY G.projects_id)AS FundingAgency,
	(SELECT I.name FROM investigator I,projectspimap PMAP WHERE I.id=PMAP.investigator_id AND PMAP.projects_id=P.id AND PMAP.role='PI') AS PI,
	AH.name AS SanctionedHead,
	(SELECT SUM(GR.amount) FROM grant_receipt GR,grant_allocation GA,grant_allocation_split GS,account_heads AHS WHERE GA.projects_id=GR.projects_id AND GR.projects_id=P.id AND GR.grant_allocation_id= GA.id AND GR.grant_allocation_split_id=GS.id AND GS.account_head_id=AH.id
       AND AHS.id=AH.id GROUP BY AHS.id)AS FundsReceived,
	(SELECT SUM(GE.expense_amount) FROM grant_expense GE,grant_allocation GL,grant_allocation_split GAS,account_heads AHS WHERE GE.grant_allocation_split_id=GAS.id AND GL.projects_id=GE.projects_id AND GE.grant_allocation_id=GL.id AND GE.projects_id=P.id AND 
	GAS.account_head_id=AHS.id AND AHS.id=AH.id GROUP BY AH.id) AS Expendeture,
	YEAR(STR_TO_DATE($P{reportDate},'%d/%M/%Y')) AS Period,
	GA.sanction_order_no AS Sancno,
	YEAR(CURDATE()) AS FinancialYear

FROM 
	projects P,
	account_heads AH,
	grant_allocation_split GS,
	grant_expense GE,
	grant_receipt GR,
	party PT,
	grant_allocation GA
WHERE 
	P.id=$P{projectID} 
	AND PT.id =$P{partyID}
	AND YEAR(GE.date_of_expense) = YEAR(STR_TO_DATE($P{reportDate},'%d/%M/%Y'))
	AND GS.account_head_id = AH.id
	-- AND GR.grant_allocation_split_id = GS.id
	-- AND GR.grant_allocation_split_id = GE.grant_allocation_split_id
	
	AND GS.projects_id = P.id
	AND GS.projects_id=GA.projects_id
	AND GA.party_id = PT.id
	AND GA.id=GE.grant_allocation_id
	AND GA.id=GS.grant_allocation_id
	
GROUP BY AH.id]]></queryString>

