	<queryString><![CDATA[SELECT 
    PR.proposal_submitteddate AS submittedDate,
    NT.notification_code AS notificationCode,
    PR.code AS proposalCode,
    P.name AS project
    
FROM
    proposal PR,
    notification NT,
    party PT,
    projects P,
    grant_allocation GA
    
WHERE
    PT.id=$P{partyID}
    AND GA.party_id=PT.id
    AND GA.projects_id=NT.project_id
    AND PR.notification_id=NT.id
    AND NT.project_id=P.id
    AND PR.lockedyn='N'
    
GROUP BY PR.code]]></queryString>

