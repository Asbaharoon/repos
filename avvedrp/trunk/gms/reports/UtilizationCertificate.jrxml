	<parameter name="projectID" isForPrompting="true" class="java.lang.String"/>
	<parameter name="grantPeriod" isForPrompting="true" class="java.lang.String"/>
	<parameter name="Path" isForPrompting="false" class="java.lang.String"/>
	<parameter name="reportDate" isForPrompting="false" class="java.util.Date"/>
	<parameter name="reportDateTo" isForPrompting="false" class="java.util.Date"/>
	<queryString><![CDATA[SELECT 
	PJ.name AS ProjectName,
	(SELECT IF ( YEAR($P{reportDate}) - YEAR($P{reportDateTo}) = 0,
         YEAR($P{reportDate}),
	 CONCAT(YEAR($P{reportDate}),'-',YEAR($P{reportDateTo}))
	 ))AS financialYear,
	(SELECT IF ( YEAR($P{reportDate}) - YEAR($P{reportDateTo}) = 0,
        YEAR($P{reportDate})+1,
        CONCAT(YEAR($P{reportDate})+1,'-',YEAR($P{reportDateTo})+1)
	))AS nextFinancialYear,
	 YEAR($P{reportDateTo}) AS currentYear,
	(SELECT GR.reference_id FROM grant_receipt GR,projects P
	WHERE GR.projects_id = P.id
	AND P.id= $P{projectID} 
	AND DATE(GR.date_of_receipt) BETWEEN $P{reportDate} AND $P{reportDateTo} GROUP BY GR.projects_id)AS LetterNo,
	(SELECT 
		SUM(total)
	 FROM 
		(SELECT 
			IF(SUM(GR.amount) IS NULL,0,SUM(GR.amount)) AS total
		FROM 
			grant_receipt GR,
			projects P 
		WHERE 
			
			GR.projects_id =  P.id
			AND (P.parent_id =$P{projectID} OR P.id =$P{projectID})
			AND DATE(GR.date_of_receipt) BETWEEN $P{reportDate} AND $P{reportDateTo} 
		GROUP BY 
			GR.projects_id)AS Amount)AS Amount,
	PY.name_of_the_institution AS Institution,
	(SELECT 
		SUM(total) 
	 FROM
			(SELECT 
				IF(SUM(GE.expense_amount) IS NULL,0,SUM(GE.expense_amount)) AS total
			FROM 
				grant_expense GE,
				-- grant_allocation_split GS,
				-- grant_period GP,
				projects P 
			WHERE 
				
				GE.projects_id =  P.id
				AND (P.parent_id =$P{projectID} OR P.id =$P{projectID})
				AND DATE(GE.date_of_expense) BETWEEN $P{reportDate} AND $P{reportDateTo}
			GROUP BY 
				GE.projects_id
			 ) AS ExpenseAmount
	 ) AS ExpenseAmount,
	 (SELECT IF(SUM(amount) IS NULL,0,SUM(amount)) FROM fund_transfer FT INNER JOIN 
          grant_allocation ON(grant_allocation.id=FT.grant_allocation_id) INNER JOIN 
          projects ON(projects.id=grant_allocation.projects_id)
         WHERE projects.parent_id= $P{projectID} AND FT.date_of_transfer BETWEEN $P{reportDate} AND $P{reportDateTo} 
	 ) AS FundAmt
FROM 
	projects PJ,
	grant_receipt GR,
	party PY,
	grant_allocation GA
WHERE 
	GA.projects_id  = PJ.id
	AND PY.id = GA.party_id
	AND (PJ.parent_id =$P{projectID} OR PJ.id =$P{projectID})
GROUP BY 
	PJ.id,GA.projects_id]]></queryString>

