	<parameter name="projectID" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT 
	PJ.name AS ProjectName,
	GA.sanction_order_no AS LetterNo,
	(SELECT 
		SUM(total)
	 FROM 
		(SELECT 
			SUM(GA.amount_allocated) AS total
		 FROM 
			grant_allocation GA
		 WHERE 
			GA.projects_id = $P{projectID}
		 GROUP BY 
			GA.projects_id)AS Amount)AS Amount,
	PY.name_of_the_institution AS Institution,
	(SELECT 
		SUM(total) 
	 FROM
			(SELECT 
				SUM(GE.expense_amount) AS total
			 FROM 
				grant_expense GE
			 WHERE 
				GE.projects_id = $P{projectID}
			 GROUP BY 
				GE.projects_id 
			 ) AS ExpenseAmount
	 ) AS ExpenseAmount
FROM 
	projects PJ,
	grant_allocation GA,
	party PY,
	grant_expense GE
WHERE 
	GA.projects_id = PJ.id
	AND PY.id = GA.party_id
	AND GA.projects_id = GE.projects_id
	AND PJ.id = $P{projectID}
GROUP BY 
	PJ.id,GE.projects_id]]></queryString>

