	<parameter name="partyID" isForPrompting="false" class="java.lang.String"/>
	<parameter name="periodID" isForPrompting="false" class="java.lang.String"/>
	<parameter name="reportDate" isForPrompting="false" class="java.lang.String"/>
	<parameter name="reportDateTo" isForPrompting="false" class="java.lang.String"/>
	<queryString><![CDATA[SELECT ga.id, ah.name  AS AcHead ,gas.amount BudjAmt,IF(SUM(ge.expense_amount) IS NULL,0,SUM(ge.expense_amount)) AS ExpAmnt ,Mp.name AS MainProjectName,p.code AS ProjectName,
pa.name_of_the_institution AS InstName,grp.name AS grantPeriod,date_of_expense,
(SELECT IF(SUM(ge.expense_amount) IS NULL,0,SUM(ge.expense_amount))  FROM grant_allocation_split gs 
LEFT JOIN grant_expense ge ON (ge.projects_id=gs.projects_id 
AND ge.grant_allocation_split_id=gs.id)
WHERE  gs.id = gas.id AND  DATE(ge.date_of_expense) BETWEEN STR_TO_DATE($P{reportDate},'%d/%M/%Y') AND STR_TO_DATE($P{reportDateTo},'%d/%M/%Y')  
GROUP BY gs.id)AS ExpAmt ,
(SELECT IF(SUM(ge.expense_amount) IS NULL,0,SUM(ge.expense_amount))  FROM grant_allocation_split gs 
LEFT JOIN grant_expense ge ON (ge.projects_id=gs.projects_id 
AND ge.grant_allocation_split_id=gs.id)INNER JOIN grant_allocation ga ON (ge.grant_allocation_id = ga.id) 
INNER JOIN party PY ON(ga.party_id = PY.id)
WHERE  gs.id = gas.id  AND  DATE(ge.date_of_expense) BETWEEN STR_TO_DATE($P{reportDate},'%d/%M/%Y') AND STR_TO_DATE($P{reportDateTo},'%d/%M/%Y')  
GROUP BY gs.id)AS ExpPartyAmt,
(SELECT IF(SUM(amount) IS NULL,0,SUM(amount)) FROM fund_transfer FT INNER JOIN 
          grant_allocation ON(grant_allocation.id=FT.grant_allocation_id) INNER JOIN 
          projects ON(projects.id=grant_allocation.projects_id)
         WHERE projects.parent_id= p.id  AND DATE(FT.date_of_transfer) BETWEEN STR_TO_DATE($P{reportDate},'%d/%M/%Y') AND STR_TO_DATE($P{reportDateTo},'%d/%M/%Y') ) AS FundAmt
FROM account_heads ah,grant_allocation_split gas,grant_expense ge,grant_allocation ga,
projects p LEFT JOIN projects Mp ON(p.parent_id=Mp.id)
 ,party pa ,grant_period grp  
WHERE ah.id= gas.account_head_id AND
gas.grant_allocation_id=ga.id AND
-- ge.grant_allocation_id=ga.id AND
ge.grant_allocation_split_id=gas.id AND
 p.id=ge.projects_id AND
pa.id=ga.party_id AND
-- Date(ge.date_of_expense) between '2009-08-01' and '2010-10-01' 
-- AND grp.id=gas.grant_period_id
-- AND gas.grant_period_id=2
( p.parent_id=$P{projectID})
GROUP BY gas.id
ORDER BY pa.id]]></queryString>

