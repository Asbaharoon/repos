INNER JOIN `general_master` ON `general_master`.id =`department_master`.`department_id` AND category='Department' and fld_value like 
$P{dept_name} 
INNER JOIN general_master A ON `A`.id =`staff_master`.`designation_id` AND A.category='Designation' INNER JOIN users ON `staff_master`.userid=users.id and users.enabled=1 AND NOT users.username='admin' AND users.username like 
$P{user_name}]]></queryString>

