	<queryString><![CDATA[SELECT users.id,`university_master`.name AS `University` , institution_master.`name` AS `Institution` , `general_master`.fld_value AS `Department`,TRIM(CONCAT(IFNULL(title,''),user_full_name,' ',IFNULL(LAST_NAME,''))) user_full_name,users.username ,users.`email`, A.fld_value AS `Designation` FROM staff_master INNER JOIN `department_master` ON `department_master`.`id`=`staff_master`.`department_id` INNER JOIN `institution_master` ON `institution_master`.id=`department_master`.`institution_id` and institution_master.name like 
$P{inst_name} 
INNER JOIN `university_master` ON `university_master`.id=`institution_master`.`university_id` AND university_master.Name like $P{univ_name} 
INNER JOIN `general_master` ON `general_master`.id =`department_master`.`department_id` AND category='Department' and fld_value like 
$P{dept_name} 
INNER JOIN general_master A ON `A`.id =`staff_master`.`designation_id` AND A.category='Designation' INNER JOIN users ON `staff_master`.userid=users.id and users.enabled=1 AND NOT users.username='admin' AND users.user_full_name like 
$P{first_name} AND users.title like $P{title} and users.last_name like $P{last_name} AND staff_master.active_yesno=1 
ORDER BY University,Institution,user_full_name]]></queryString>

