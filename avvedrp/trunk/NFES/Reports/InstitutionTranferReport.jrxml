	<parameter name="department" isForPrompting="true" class="java.lang.String"/>
	<parameter name="firstname" isForPrompting="true" class="java.lang.String"/>
	<parameter name="nametitle" isForPrompting="true" class="java.lang.String"/>
	<parameter name="lastname" isForPrompting="true" class="java.lang.String"/>
	<parameter name="header_image" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT  `university_master`.name AS 'University', 
TRIM(CONCAT(users.title,' ',users.user_full_name,' ',users.last_name)) AS 'faculty_name',
institution_master.name AS 'Institution',
general_master.fld_value AS 'department' ,
staff_master.join_date ,staff_master.relieve_date,Tbl_designation.fld_value AS 'designation'
FROM staff_master
INNER JOIN department_master ON staff_master.department_id =department_master.id
INNER JOIN `general_master` ON general_master.id=department_master.department_id AND category='department'  AND general_master.fld_value LIKE $P{department}
INNER JOIN `institution_master` ON `institution_master`.id=`department_master`.`institution_id` AND institution_master.name LIKE $P{institution}
INNER JOIN `university_master` ON `university_master`.id=`institution_master`.`university_id` AND university_master.name LIKE $P{university}
INNER JOIN users ON users.id=`staff_master`.`userid` 
AND users.title LIKE $P{nametitle}
AND users.user_full_name LIKE $P{firstname}
AND users.last_name LIKE $P{lastname}
INNER JOIN general_master AS Tbl_designation 
ON Tbl_designation.id=staff_master.designation_id  AND Tbl_designation.category='designation'
WHERE record_staus='T'
ORDER BY university_master.name,users.id,staff_master.id]]></queryString>

