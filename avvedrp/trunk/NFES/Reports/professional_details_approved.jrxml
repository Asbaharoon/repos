	<parameter name="lastname" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT staff_profile_masterdetails_v0_values.idf,full_name, 
 staff_profile_masterdetails_v0_values.university_id,staff_profile_masterdetails_v0_values.university, 
 staff_profile_masterdetails_v0_values.institution_id ,staff_profile_masterdetails_v0_values.institution   ,
staff_profile_masterdetails_v0_values.department,
entity_document_master.number, `entity_document_master`.`approved_yesno`,
entity_document_master.`approved_by`,
DATE(entity_document_master.`approved_date`) approved_date,
entity_document_master.document_id,entity_document_master.entity_type,
entity_document_master.public_record,
focus_area,
research_interests,
teaching_interests,
positions_held 
FROM staff_profile_personal_details_v0_values 
INNER JOIN `staff_profile_masterdetails_v0_values` ON `staff_profile_personal_details_v0_values`.idf=staff_profile_masterdetails_v0_values.userid   
INNER JOIN `entity_document_master` ON 
entity_document_master.`entity_id`=staff_profile_personal_details_v0_values.idf
AND entity_document_master.number=staff_profile_personal_details_v0_values.number 
AND entity_document_master.`entity_type`='personal_details'
AND `entity_document_master`.`approved_yesno`=1
AND staff_profile_masterdetails_v0_values.user_full_name LIKE $P{facultyname}
AND staff_profile_masterdetails_v0_values.university LIKE $P{university}
AND staff_profile_masterdetails_v0_values.institution LIKE $P{institution}
AND staff_profile_masterdetails_v0_values.department LIKE $P{department}
AND staff_profile_masterdetails_v0_values.user_title LIKE $P{usertitle}
AND staff_profile_masterdetails_v0_values.last_name LIKE $P{lastname}
AND DATE_FORMAT(approved_date,'%Y-%m-%d') BETWEEN $P{fromdate} AND $P{todate};]]></queryString>

